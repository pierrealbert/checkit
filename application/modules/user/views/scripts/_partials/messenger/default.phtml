<? if (!empty($this->messages)) : ?>
    <script type="text/javascript">
    <? foreach ($this->messages as $type => $messages) : ?>
        <? foreach ($messages as $message) : ?>
        addToMessagesQueue('<?=addslashes($this->translate($type))?>', '<?=addslashes($message)?>');
        <? endforeach ?>
    <? endforeach; ?>
    </script>
<? endif; ?>